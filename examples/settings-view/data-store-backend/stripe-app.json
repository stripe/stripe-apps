{
  "id": "com.example.settings-view",
  "version": "0.0.1",
  "name": "Settings View Example",
  "icon": "",
  "permissions": [
    {
      "permission": "datastore_write",
      "purpose": "For storing additional data in Stripe"
    }
  ],
  "ui_extension": {
    "views": [
      {
        "viewport": "stripe.dashboard.drawer.default",
        "component": "App"
      },
      {
        "viewport": "settings",
        "component": "AppSettings"
      }
    ],
    "actions": [],
    "content_security_policy": {
      "connect-src": null,
      "image-src": null,
      "purpose": ""
    }
  }
}