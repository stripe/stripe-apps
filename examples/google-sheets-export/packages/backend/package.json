{
  "name": "backend",
  "version": "0.0.1",
  "description": "Authenticates users with Google Sheets via OAuth",
  "private": true,
  "license": "~~proprietary~~",
  "main": ".build/index.js",
  "scripts": {
    "build": "tsc && copyfiles -f ./src/views/* ./.build/views",
    "start": "node .build/index.js",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "dev": "nodemon ./src/index.ts",
    "generateKeys": "openssl req -nodes -new -x509 -keyout .build/server.key -out .build/server.cert",
    "test": "jest src",
    "test:watch": "npm test -- --watch"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0"
  },
  "dependencies": {
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "googleapis": "^95.0.0",
    "stripe": "^8.203.0",
    "typescript": "^4.5.5"
  }
}
