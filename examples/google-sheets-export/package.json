{
  "name": "com.example.google-sheets",
  "version": "0.0.1",
  "description": "Allows users to export their Stripe data to Google Sheets",
  "private": true,
  "license": "~~proprietary~~",
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@stripe/ui-extension-sdk": "^3.0.0",
    "axios": "^0.25.0",
    "crypto": "^1.0.1",
    "date-fns": "^2.28.0",
    "ejs": "^3.1.6",
    "react": "^17.0.2",
    "stripe": "^8.198.0"
  },
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "start": "npm start --workspace=frontend",
    "start:backend": "npm run dev --workspace=backend",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.jsx,.tsx .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|jsx|tsx|json)\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --cache --fix --ignore-path .gitignore",
    "*.+(js|ts|jsx|tsx|json)": "prettier --write --ignore-path .gitignore"
  }
}
