{
  "id": "com.example.accessing-contextual-data",
  "version": "0.0.1",
  "name": "Accessing Contextual Data",
  "icon": "",
  "permissions": [
    {
      "permission": "customer_read",
      "purpose": "Allows this example app to read details of a customer"
    },
    {
      "permission": "payment_intent_read",
      "purpose": "Allows this example app to read details of a payment"
    },
    {
      "permission": "invoice_read",
      "purpose": "Allows this example app to read details of an invoice"
    },
    {
      "permission": "product_read",
      "purpose": "Allows this example app to read details of a product"
    },
    {
      "permission": "subscription_read",
      "purpose": "Allows this example app to read details of a subscription"
    }
  ],
  "ui_extension": {
    "views": [
      {
        "viewport": "stripe.dashboard.customer.detail",
        "component": "Customer"
      },
      {
        "viewport": "stripe.dashboard.payment.detail",
        "component": "Payment"
      },
      {
        "viewport": "stripe.dashboard.invoice.detail",
        "component": "Invoice"
      },
      {
        "viewport": "stripe.dashboard.product.detail",
        "component": "Product"
      },
      {
        "viewport": "stripe.dashboard.subscription.detail",
        "component": "Subscription"
      }
    ],
    "actions": [],
    "content_security_policy": {
      "connect-src": null,
      "image-src": null,
      "purpose": ""
    }
  }
}